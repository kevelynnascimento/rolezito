{
  "name": "rolezito-api",
  "version": "1.0.0",
  "description": "API REST para o app Rolezito",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/database.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create src/migrations/",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/config/database.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.3",
    "routing-controllers-openapi": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.10.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
